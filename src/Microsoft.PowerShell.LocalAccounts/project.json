{
    "name": "Microsoft.PowerShell.LocalAccounts",
    "version": "6.0.0-*",

    "buildOptions": {
        "xmlDoc": true,
        "keyFile": "../signing/visualstudiopublic.snk",
        "delaySign": true,
        "publicSign": false,
        "warningsAsErrors": true
    },

    "dependencies": {
        "System.Management.Automation": "6.0.0-*"
    },

    "frameworks": {
        "netstandard1.6": {
            "imports": [ "dnxcore50" ],
            "buildOptions": {
                "define": [ "CORECLR" ],
                "debugType": "portable"
            },
            "dependencies": {
                "System.Net.NameResolution": "4.3.0-preview1-24530-04"
            }
        },
        "net451": {
        }
    }
}
