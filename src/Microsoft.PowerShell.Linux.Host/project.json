{
    "name": "powershell",
    "version": "1.0.0-*",
    "description": "PowerShell On Linux Console",
    "authors": [ "andschwa" ],

    "compilationOptions": {
        "emitEntryPoint": true
    },

    "dependencies": {
        "Newtonsoft.Json": "8.0.2",
        "Microsoft.PowerShell.Commands.Management": {
            "type": "build",
            "version": "1.0.0-*"
