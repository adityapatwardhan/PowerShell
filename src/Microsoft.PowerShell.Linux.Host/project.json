{
    "name": "powershell",
    "version": "1.0.0-*",
    "description": "PowerShell Managed Console for .NET Core",
    "authors": [ "andschwa" ],

    "compilationOptions": {
        "warningsAsErrors": true,
        "emitEntryPoint": true
    },

    "dependencies": {
        "Newtonsoft.Json": "8.0.2",
        "System.Xml.XDocument": "4.0.11-rc3-23931-00",
        "System.IO.MemoryMappedFiles": "4.0.0-rc3-23931-00",
        "Microsoft.PowerShell.Commands.Management": "1.0.0-*",
        "Microsoft.PowerShell.Commands.Utility": "1.0.0-*"
    },

    "content": [
        "Modules",
        "*_profile.ps1",
        "*.so",
        "*.dylib"
    ],

    "frameworks": {
        "netstandardapp1.5": {
            "imports": [ "dnxcore50", "portable-net45+win8" ]
        }
    }
}
